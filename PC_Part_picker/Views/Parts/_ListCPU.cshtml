@model PC_Part_picker.Models.PartViewModel

<p>
    <a asp-action="_CreateCPU">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Rating)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Manufacturer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Color)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Cores)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Frequency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Series)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPU.First().Consumption)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.CPU) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Manufacturer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Color)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cores)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Frequency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Series)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Consumption)
            </td>
            <td>
                <a asp-action="_EditCPU" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="_DetailsCPU" asp-route-id="@item.Id">Details</a> |
                <a asp-action="_DeleteCPU" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
